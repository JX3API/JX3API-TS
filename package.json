{
  "name": "jx3api-ts",
  "version": "2.1.0",
  "description": "剑网3 API 的 TypeScript SDK，提供了一套完整的接口封装",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "browser": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "node": ">=22.12.0",
  "scripts": {
    "build": "cross-env NODE_ENV=production type=ESM rollup -c",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish"
  },
  "lint-staged": {
    "**/*.ts": [
      "npm run lint",
      "prettier --write"
    ]
  },
  "keywords": [
    "jx3",
    "api",
    "sdk",
    "剑网3",
    "游戏数据",
    "剑网3API",
    "剑网3SDK"
  ],
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "author": {
    "name": "Fine",
    "email": "fine_951111@163.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JX3API/JX3API-TS"
  },
  "bugs": {
    "url": "https://github.com/JX3API/JX3API-TS/issues"
  },
  "homepage": "https://github.com/JX3API/JX3API-TS#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.24.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.5.3",
    "rollup": "^4.28.1",
    "rollup-plugin-delete": "^2.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  },
  "type": "module",
  "dependencies": {
    "@types/ws": "^8.18.1",
    "axios": "^1.12.1",
    "ws": "^8.18.1"
  }
